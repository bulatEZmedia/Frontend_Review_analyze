<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context=".ChoiceAnalyzeActivity"
    android:background="@drawable/background">


    <com.google.android.material.button.MaterialButton
        android:id="@+id/then2"
        style="@style/Widget.MaterialComponents.Button.OutlinedButton"
        android:layout_width="343dp"
        android:layout_height="67dp"
        android:layout_marginTop="256dp"
        android:backgroundTint="#090909"
        android:text="Основные недостатки"
        android:textAllCaps="false"
        android:textColor="#DAEA25"
        android:textSize="16sp"
        app:cornerRadius="35dp"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent" />

    <com.google.android.material.button.MaterialButton
        android:id="@+id/then3"
        style="@style/Widget.MaterialComponents.Button.OutlinedButton"
        android:layout_width="343dp"
        android:layout_height="67dp"
        android:layout_marginTop="16dp"
        android:backgroundTint="#090909"
        android:text="Методы решения"
        android:textAllCaps="false"
        android:textColor="#DAEA25"
        android:textSize="16sp"
        app:cornerRadius="35dp"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/then2" />

    <com.google.android.material.button.MaterialButton
        android:id="@+id/then4"
        style="@style/Widget.MaterialComponents.Button.OutlinedButton"
        android:layout_width="343dp"
        android:layout_height="67dp"
        android:layout_marginTop="16dp"
        android:backgroundTint="#090909"
        android:text="Преимущества"
        android:textAllCaps="false"
        android:textColor="#DAEA25"
        android:textSize="16sp"
        app:cornerRadius="35dp"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/then3" />

    <View
        android:id="@+id/myRectangleView2"
        android:layout_width="match_parent"
        android:layout_height="120dp"
        android:layout_alignParentStart="true"
        android:layout_alignParentTop="true"
        android:layout_alignParentEnd="true"
        android:layout_marginBottom="644dp"
        android:background="@drawable/rectangle4"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="1.0"
        app:layout_constraintStart_toStartOf="parent" />

    <TextView
        android:id="@+id/textView3"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_alignParentTop="true"
        android:layout_marginStart="20dp"
        android:layout_marginTop="28dp"
        android:text="Анализ выполнен"
        android:textColor="#F7F8FA"
        android:textSize="20sp"
        app:layout_constraintStart_toEndOf="@+id/imageView"
        app:layout_constraintTop_toTopOf="parent" />

    <ImageView
        android:id="@+id/imageView"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginStart="28dp"
        android:layout_marginTop="16dp"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:srcCompat="@drawable/logo" />

    <TextView
        android:id="@+id/textView4"
        android:layout_width="33dp"
        android:layout_height="15dp"
        android:layout_marginTop="15dp"
        android:text="Babich"
        android:textColor="#DEFA2C"
        android:textSize="10sp"
        android:textStyle="bold"
        app:layout_constraintEnd_toEndOf="@+id/imageView"
        app:layout_constraintStart_toStartOf="@+id/imageView"
        app:layout_constraintTop_toTopOf="@+id/imageView" />

    <ImageView
        android:id="@+id/imageView3"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginEnd="24dp"
        app:layout_constraintBottom_toBottomOf="@+id/then2"
        app:layout_constraintEnd_toEndOf="@+id/then2"
        app:layout_constraintTop_toTopOf="@+id/then2"
        app:layout_constraintVertical_bias="0.477"
        app:srcCompat="@drawable/go" />

    <ImageView
        android:id="@+id/imageView4"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginEnd="24dp"
        app:layout_constraintBottom_toBottomOf="@+id/then3"
        app:layout_constraintEnd_toEndOf="@+id/then3"
        app:layout_constraintTop_toTopOf="@+id/then3"
        app:layout_constraintVertical_bias="0.477"
        app:srcCompat="@drawable/go" />

    <ImageView
        android:id="@+id/imageView5"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginEnd="24dp"
        app:layout_constraintBottom_toBottomOf="@+id/then4"
        app:layout_constraintEnd_toEndOf="@+id/then4"
        app:layout_constraintTop_toTopOf="@+id/then4"
        app:layout_constraintVertical_bias="0.477"
        app:srcCompat="@drawable/go" />
</androidx.constraintlayout.widget.ConstraintLayout>